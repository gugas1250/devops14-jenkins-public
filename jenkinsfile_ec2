pipeline {
    agent any
    stages {
        stage ('Checkout') {
            steps {
                git branch: 'main', url: 'git@github.com:gugas1250/devops14-jenkins-public.git'
            }
    }
    stage ("CreateEnv") {
    steps {
        script {
            sh "chmod =x -R ${env.WORKSPACE}/../${env.JOB_NAME}ec2.sh"
        }
    }
}
}
}
